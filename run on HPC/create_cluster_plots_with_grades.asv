function [] = create_cluster_plots_with_grades(parent_dir_of_data_saving,data_to_load,name_of_dir_to_save_files_to)
home_dir = cd("..");
addpath(genpath(pwd));
cd(home_dir);
load(data_to_load,"table_of_cluster_classification")
home_dir = cd(parent_dir_of_data_saving);
dir_for_figures_to_be_saved_to = create_a_file_if_it_doesnt_exist_and_ret_abs_path(name_of_dir_to_save_files_to);

cd(home_dir);
end