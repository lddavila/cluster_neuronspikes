    elseif grades(26) > 21000000
        current_cluster_category = "Neuron c7";
    elseif grades(28) > -0.1 && grades(28) <= 0.1 
        current_cluster_category = "Neuron c8";
    elseif grades(30) <= 1.1 && grades(30) > 1 
        current_cluster_category = "Neuron c9";
    elseif grades(29) < 2e-30 
        current_cluster_category = "Neuron c10";
    elseif grades(40) >= 0.5
        current_cluster_category = "Neuron c11";
    elseif grades(36) < 0.2 
        current_cluster_category = "Neuron c12";
    end
    if current_cluster_category==""
        current_cluster_category = "MUA";
    end
end
