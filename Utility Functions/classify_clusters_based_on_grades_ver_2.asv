function [current_cluster_category] = classify_clusters_based_on_grades_ver_2(grades)
current_cluster_category = "";
    if grades(5) < 550
        if grades(2)> 0.0538 && grades(2) < 0.07
            current_cluster_category = "Neuron 1.1";
        elseif grades(6) > grades(2670)
            current_cluster_category = "Neuron 1.2";
        elseif grades(12) > 0.086 && grades(0.13)
            current_cluster_category = "Neuron c1.3";
        elseif grades(13) < 0.344 && grades(13) > 0.086
            current_cluster_category = "Neuron c1.4";
         elseif grades(14) <0.303  && grades(17) > 0.101
            current_cluster_category = "Neuron c1.5";   
        elseif grades(25) <92
            current_cluster_category = "MUA c1.6";   
        elseif grades(28) >0.204
            current_cluster_category = "MUA c1.7";
        elseif grades(36) >0.25
            current_cluster_category = "Neuron c1.8";
        elseif grades() >  && grades() <
            current_cluster_category = "Neuron c1.";
        end
        
    elseif grades(12) < 0.57
        current_cluster_category = "Neuron c2";
    elseif grades(13) < 1 && grades(13) > 0.05
        current_cluster_category = "Neuron c3";
    elseif grades(18) > 0 && grades(18) < 0.03 
